{
  "name": "poketify",
  "description": "Pokemon API built with Fastify.",
  "homepage": "https://fastify.dev",
  "version": "0.0.1",
  "main": "app.ts",
  "engines": {
    "node": ">=18"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/app.ts",
    "build": "pnpm run build:pre && pnpm run build:compile",
    "start": "node dist/main.js",
    "lint": "eslint --ext .ts,.js,.json,.tsx src/",
    "lint:fix": "eslint --fix --ext .ts,.js,.json,.tsx src/",
    "build:pre": "rm -rf dist/",
    "build:compile": "tsc && tsc-alias",
    "preinstall": "npx -y only-allow pnpm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^6.0.0",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "c8": "^7.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fastify-tsconfig": "^2.0.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}